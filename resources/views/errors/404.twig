{% extends "layout" %}
{% block content %}
<div id="content-wrapper">
  <section class="container error-page">
    <div class="error-page__title-container">
      <h2 class="error-page__title">404</h2>
    </div>
    <h3 class="error-page__subtitle">Oops.. Page not found !</h3>
